        "mockjs": "^1.1.0",
        "pinia": "^2.1.7",
        "pinia-plugin-persistedstate": "^3.2.1",
        "vite-plugin-importer": "^0.2.5",
        "vite-plugin-require-transform": "^1.0.21",
